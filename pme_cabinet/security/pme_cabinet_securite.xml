<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1">

    <record model="ir.module.category" id="module_category_pme_cabinet">
        <field name="name">PME Cabinet</field>
        <field name="description">Helps you manage your PME and Cabinet.</field>
        <field name="sequence">17</field>
    </record>

    <record id="group_pme_cabinet_user" model="res.groups">
        <field name="name">Officer</field>
        <field name="category_id" ref="pme_cabinet.module_category_pme_cabinet"/>
        <field name="implied_ids" eval="[(4, ref('hr.group_hr_user')), (4, ref('hr_contract.group_hr_contract_manager'))]"/>
    </record>

    <record id="group_pme_cabinet_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="pme_cabinet.module_category_pme_cabinet"/>
        <field name="implied_ids" eval="[(4, ref('pme_cabinet.group_pme_cabinet_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>


    </data>
</odoo>
